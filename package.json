{
  "name": "cypress-metamask",
  "version": "1.0.6-dev",
  "main": "support/commands.js",
  "license": "MIT",
  "keywords": [
    "Cypress",
    "Testing",
    "EthersJS",
    "Smart Contracts",
    "Blockchain",
    "MetaMask",
    "Utils"
  ],
  "standard": {
    "env": [
      "cypress/globals"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "cypress"
    ]
  },
  "scripts": {
    "start": "superstatic src",
    "start:silent": "BROWSER=none superstatic src",
    "cy:open": "CYPRESS_REMOTE_DEBUGGING_PORT=9222 cypress open",
    "cy:run": "CYPRESS_REMOTE_DEBUGGING_PORT=9222 cypress run --headed --browser chrome"
  },
  "dependencies": {
    "cross-zip": "^4.0.0",
    "cypress": "^12.17.4",
    "dotenv": "^9.0.2",
    "eslint-plugin-cypress": "^2.11.3",
    "ethers": "^5.1.4",
    "node-fetch": "^2.6.1",
    "puppeteer-core": "^22.11.2",
    "standard": "^17.1.0",
    "start-server-and-test": "^2.0.2",
    "superstatic": "^9.0.1",
    "semantic-release": "^24.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gitcoinco/cypress-metamask.git"
  }
}
